<mujoco model="pr2_model">
    <worldbody name="base">
        <body name="pr2" pos="0 0 0.5">
            <joint axis="1 0 0" name="robot_x" type="slide" pos="0 0 0"/>
            <joint axis="0 1 0" name="robot_y" type="slide" pos="0 0 0"/>
            <joint axis="0 0 1" name="robot_theta" type="hinge" pos="0 0 0" limited="true" range="-3.15 3.15"/>
            <geom name="pr2" type="cylinder" size="0.275 0.45" />
            <inertial pos="0 0 0" mass="10" diaginertia="0.001 0.001 0.001" />
            <body name="right_finger" pos="-0.35 0.7 0" euler="0 0 -0.05">
                <inertial pos="0 0 0" mass="2" diaginertia="0.001 0.001 0.001" />
                <joint name="right_finger_joint" axis="1 0 0" limited="true" range="-0.1 0.1" type="slide" />
                <geom pos="0 0 0" size="0.05 0.3495 0.45" type="box" friction="1.5 1 0.5" />
            </body>
            <body name="left_finger" pos="0.35 0.7 0" euler="0 0 0.05">
                <inertial pos="0 0 0" mass="2" diaginertia="0.001 0.001 0.001" />
                <joint name="left_finger_joint" axis="-1 0 0" limited="true" range="-0.1 0.1" type="slide" />
                <geom pos="0 0 0" size="0.05 0.3495 0.45" type="box" friction="1.5 1 0.5" />
            </body>
        </body>
    </worldbody>

    <actuator>
        <velocity gear="1" kv="2000" joint="robot_x" name="robot_x" ctrllimited="true" ctrlrange="-50 50" />
        <velocity gear="1" kv="2000" joint="robot_y" name="robot_y" ctrllimited="true" ctrlrange="-50 50" />
        <velocity gear="1" kv="200" joint="robot_theta" name="robot_theta" ctrllimited="true" ctrlrange="-20 20" />
        <velocity gear="1" kv="100" joint="left_finger_joint" name="left_grip" ctrllimited="true" ctrlrange="-20 20" />
        <velocity gear="1" kv="100" joint="right_finger_joint" name="right_grip" ctrllimited="true" ctrlrange="-20 20" />
    </actuator>
</mujoco>
