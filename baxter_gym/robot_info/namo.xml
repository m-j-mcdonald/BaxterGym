<mujoco model="pr2">
    <worldbody name="base">
        <body name="pr2">
            <joint axis="1 0 0" name="robot_x" type="slide" pos="0 0 0"/>
            <joint axis="0 1 0" name="robot_y" type="slide" pos="0 0 0"/>
            <geom name="pr2" type="cylinder" size="0.3 1" friction="1 1 1" />
            <intertial pos="0 0 0" mass="10" diaginertia="0.001 0.001 0.001" />
            <body name="right_finger" pos="0.55 -0.3 0">
                <joint name="right_finger_joint" axis="0 1 0" limited="true" range="0 0.1" type="slide" />
                <geom pos="0 0 0" size="0.225 0.025 0.5" type="box" />
            </body>
            <body name="left_finger" pos="0.55 0.3 0">
                <joint name="left_finger_joint" axis="0 -1 0" limited="true" range="0 0.1" type="slide" />
                <geom pos="0 0 0" size="0.225 0.025 0.5" type="box" />
            </body>
        </body>
    </worldbody>

    <actuator>
        <position gear="1" kp="1000" joint="robot_x" name="robot_x" forcelimited="true" forcerange="-500 500" />
        <position gear="1" kp="1000" joint="robot_y" name="robot_y" forcelimited="true" forcerange="-500 500" />
        <position gear="1" kp="1000" joint="left_finger_joint" name="left_grip" forcelimited="true" forcerange="-500 500" />
        <position gear="1" kp="1000" joint="right_finger_joint" name="right_grip" forcelimited="true" forcerange="-500 500" />
    </actuator>
</mujoco>
